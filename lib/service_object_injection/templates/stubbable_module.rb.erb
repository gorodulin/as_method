
module <%= module_name %>
  PARENT = ::<%= object %>
  private_constant :PARENT

  def self.ruby2_keywords(*); end if RUBY_VERSION < "2.7"

  def self.call(*args, &block)
    (@stub || PARENT).call(*args, &block)
  end

  def self.get_stub
    @stub
  end

  def self.set_stub(stub)
    @stub = stub
  end

  def self.unstub!
    @stub = nil
  end
end
